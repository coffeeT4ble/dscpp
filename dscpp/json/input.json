{
    "Tables": {
        "Members": {
            "Columns": {
                "MemberID": {
                    "Type": "INT",
                    "PrimaryKey": true,
                    "AutoIncrement": true
                },
                "FirstName": {
                    "Type": "NVARCHAR(100)",
                    "NotNull": true
                },
                "LastName": {
                    "Type": "NVARCHAR(100)",
                    "NotNull": true
                },
                "Email": {
                    "Type": "NVARCHAR(100)",
                    "NotNull": true,
                    "Unique": true
                },
                "PhoneNumber": {
                    "Type": "NVARCHAR(15)"
                },
                "DateOfBirth": {
                    "Type": "DATE"
                },
                "MembershipID": {
                    "Type": "INT",
                    "ForeignKey": {
                        "References": "Memberships(MembershipID)"
                    }
                },
                "JoinDate": {
                    "Type": "DATE",
                    "NotNull": true
                }
            }
        },
        "Memberships": {
            "Columns": {
                "MembershipID": {
                    "Type": "INT",
                    "PrimaryKey": true,
                    "AutoIncrement": true
                },
                "MembershipType": {
                    "Type": "NVARCHAR(50)",
                    "NotNull": true
                },
                "DurationInMonths": {
                    "Type": "INT",
                    "NotNull": true
                },
                "Price": {
                    "Type": "DECIMAL(10, 2)",
                    "NotNull": true
                },
                "Benefits": {
                    "Type": "NVARCHAR(MAX)"
                }
            }
        },
        "Trainers": {
            "Columns": {
                "TrainerID": {
                    "Type": "INT",
                    "PrimaryKey": true,
                    "AutoIncrement": true
                },
                "FirstName": {
                    "Type": "NVARCHAR(100)",
                    "NotNull": true
                },
                "LastName": {
                    "Type": "NVARCHAR(100)",
                    "NotNull": true
                },
                "Specialization": {
                    "Type": "NVARCHAR(100)"
                },
                "Email": {
                    "Type": "NVARCHAR(100)",
                    "NotNull": true,
                    "Unique": true
                },
                "PhoneNumber": {
                    "Type": "NVARCHAR(15)"
                },
                "HireDate": {
                    "Type": "DATE",
                    "NotNull": true
                }
            }
        },
        "Classes": {
            "Columns": {
                "ClassID": {
                    "Type": "INT",
                    "PrimaryKey": true,
                    "AutoIncrement": true
                },
                "ClassName": {
                    "Type": "NVARCHAR(100)",
                    "NotNull": true
                },
                "Description": {
                    "Type": "NVARCHAR(MAX)"
                },
                "Schedule": {
                    "Type": "DATETIME",
                    "NotNull": true
                },
                "TrainerID": {
                    "Type": "INT",
                    "ForeignKey": {
                        "References": "Trainers(TrainerID)"
                    }
                },
                "Capacity": {
                    "Type": "INT",
                    "NotNull": true
                }
            }
        },
        "Attendance": {
            "Columns": {
                "AttendanceID": {
                    "Type": "INT",
                    "PrimaryKey": true,
                    "AutoIncrement": true
                },
                "MemberID": {
                    "Type": "INT",
                    "ForeignKey": {
                        "References": "Members(MemberID)"
                    }
                },
                "ClassID": {
                    "Type": "INT",
                    "ForeignKey": {
                        "References": "Classes(ClassID)"
                    }
                },
                "AttendanceDate": {
                    "Type": "DATETIME",
                    "NotNull": true
                },
                "Status": {
                    "Type": "NVARCHAR(10)",
                    "Check": [ "Present", "Absent" ]
                }
            }
        }
    }
}
